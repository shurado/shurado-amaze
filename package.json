  "main": "index.js",
  "scripts": {
    "prestart": "npm run -s build:server",
    "start": "npm run dev:server",
    "dev:server": "BABEL_ENV=production NODE_ENV=development nodemon --watch ./src --exec \"node -r babel-register ./src/index.js\"",
    "dev:client": "nodemon --watch webpack.config.js --watch webpack.parts.js --exec \"webpack-dev-server --env.target development\"",
    "clean": "rm -rf ./dist",
    "build:webpack": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.js  --env.target production",
    "build:server": "npm run clean && mkdir -p dist && babel --no-babelrc src -s -D -d dist --presets=react,es2015",
    "lint:js": "git diff --name-only --diff-filter=ACMRTUB `git merge-base HEAD master` | grep '\\.js$' | xargs eslint",
    "lint:style": "stylelint ./client/styles/**/*.scss",
    "test": "jest --watch ./src",
    "test:client": "jest --watch ./client",
    "testfile": "jest",
    "prod:server": "node dist/index.js",
    "migrate": "sequelize db:migrate",
    "rollback": "sequelize db:migrate:undo:all"
  },
